<h1> Edit Sitter </h1>
<%= form_for (@sitter) do |s| %>
<%= s.label :name %>
<%= s.text_field :name %></br>
<%= s.label :email %>
<%= s.email_field :email %>
<%= s.label :age %>
<%= s.text_field :age %></br>
<%= s.label :zip %>
<%= s.text_field :zip %>
<%= s.label :ph_num %>
<%= s.text_field :ph_num %></br>
<%= s.submit %>
<% end %>
