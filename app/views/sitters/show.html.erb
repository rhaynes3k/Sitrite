<h1> <%= @sitter.name %> Sitter Page </h1>
<p>
  <strong> Name: </strong></br>
  <%= @sitter.name %></br>
  <strong> Age: </strong>
  <%= @sitter.age %></br>
  <strong> Zip Code: </strong></br>
  <%= @sitter.zip %></br>
  <strong> Email: </strong>
  <%= @sitter.email %></br>
  <strong> Ph Num: </strong>
  <%= @sitter.ph_num %></br>
  <p><strong> Jobs: </strong></br>
    <% @sitter.jobs.map do|j| %>
    <strong><%= j.date.strftime("%m/%d/%Y") if j.valid? %></br></strong>
      <%= link_to j.guardian.name, guardian_path(j.guardian.id) if j.valid? %></br>
    <% end %>
  </p>
  <strong><%= link_to('Edit', edit_sitter_path(@sitter.id)) %></br></strong>
  <strong><%= link_to "Delete", sitter_path(@sitter.id), data: { confirm: "Are you sure?" }, :method => :delete %></strong>
